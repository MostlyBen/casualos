{
    "name": "@casual-simulation/casual-apiary-aws",
    "version": "1.0.0",
    "private": true,
    "description": "Serverless CasualOS on AWS",
    "main": "handler.js",
    "scripts": {
        "start": "sam local start-api -p 2998 --docker-network docker_default --env-vars env.json --env-vars secrets.env.json --log-file ./debug_logs.txt",
        "build": "node ./scripts/build-apiary-aws.js",
        "test:watch": "jest --watchAll",
        "test": "jest --verbose",
        "test:ci": "jest --no-cache --ci --reporters=default",
        "dynamodb:install": "serverless dynamodb install --stage dev",
        "deploy:dev": "serverless deploy --stage dev",
        "deploy:casualos-dev": "serverless deploy --stage casualos-dev",
        "deploy:boormanlabs": "serverless deploy --stage boormanlabs",
        "deploy:casualos": "serverless deploy --stage casualos-redis",
        "deploy:publicos": "serverless deploy --stage publicos-prod",
        "deploy:auxbackend": "serverless deploy --stage auxbackend-prod"
    },
    "dependencies": {
        "@casual-simulation/aux-common": "^3.0.4",
        "@casual-simulation/aux-vm": "^3.0.4",
        "@casual-simulation/causal-trees": "^3.0.2",
        "yjs": "13.5.41",
        "aws-sdk": "2.789.0",
        "axios": "0.21.0",
        "base64-js": "^1.5.1",
        "fast-json-stable-stringify": "2.1.0",
        "hash.js": "1.1.7",
        "lodash": "4.17.20",
        "rxjs": "6.6.3",
        "scrypt-js": "3.0.1",
        "source-map-support": "^0.5.10",
        "tweetnacl": "1.0.3",
        "uuid": "^8.3.2",
        "redis": "3.0.2"
    },
    "devDependencies": {},
    "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
    "license": "MIT"
}
